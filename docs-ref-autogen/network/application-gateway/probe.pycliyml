- commandList: []
  htmlId: create
  examples:
  - title: Create an application gateway probe.
    code: az network application-gateway probe create -g MyResourceGroup -n MyProbe --protocol Https --gateway-name MyApplicationGateway --host 127.0.0.1 --path /path/to/probe
  parameters:
  - name: --gateway-name
    isRequired: True
    summary: The name of the application gateway.
    description: ''
  - name: --host
    isRequired: True
    summary: The name of the host to send the probe.
    description: ''
  - name: --name -n
    isRequired: True
    summary: The name of the probe.
    description: ''
  - name: --path
    isRequired: True
    summary: The relative path of the probe. Valid paths start from "/".
    description: ''
  - name: --protocol
    isRequired: True
    summary: The HTTP settings protocol.
    description: ''
    parameterValueGroup: Http, Https
  - name: --resource-group -g
    isRequired: True
    summary: Name of resource group. You can configure the default group using az configure --defaults group=<name>.
    description: ''
  - name: --interval
    isRequired: False
    summary: The time interval in seconds between consecutive probes.
    description: ''
    defaultValue: 30
  - name: --no-wait
    isRequired: False
    summary: Do not wait for the long running operation to finish.
    description: ''
  - name: --threshold
    isRequired: False
    summary: The number of failed probes after which the back end server is marked down.
    description: ''
    defaultValue: 8
  - name: --timeout
    isRequired: False
    summary: The probe timeout in seconds.
    description: ''
    defaultValue: 120
  - name: --debug
    isRequired: False
    summary: Increase logging verbosity to show all debug logs.
    description: ''
  - name: --help -h
    isRequired: False
    summary: Show this help message and exit.
    description: ''
  - name: --output -o
    isRequired: False
    summary: Output format.
    description: ''
    defaultValue: json
    parameterValueGroup: json, jsonc, table, tsv
  - name: --query
    isRequired: False
    summary: JMESPath query string. See <a href="http://jmespath.org/">http://jmespath.org/</a> for more information and examples.
    description: ''
  - name: --verbose
    isRequired: False
    summary: Increase logging verbosity. Use --debug for full debug logs.
    description: ''
  source:
