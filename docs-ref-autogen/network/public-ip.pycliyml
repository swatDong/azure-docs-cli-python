    description: ''
  - name: --reverse-fqdn
    isRequired: False
    summary: Reverse FQDN (fully qualified domain name).
    description: ''
  - name: --tags
    isRequired: False
    summary: Space separated tags in 'key[=value]' format. Use "" to clear existing tags.
    description: ''
  - name: --version
    isRequired: False
    summary: IP address type.
    description: ''
    defaultValue: IPv4
    parameterValueGroup: IPv4, IPv6
